    "GalaxyInfoModel": {
      "title": "GalaxyInfoModel",
      "type": "object",
      "allOf": [
        {
          "if": {
            "properties": {
              "standalone": {
                "const": true
              }
            }
          },
          "then": {
            "$comment": "Standalone role, so we require several fields.",
            "required": [
              "author",
              "description",
              "license",
              "min_ansible_version"
            ]
          }
        },
        {
          "if": {
            "properties": {
              "standalone": {
                "const": false
              }
            }
          },
          "then": {
            "$comment": "Collection roles do not use most galaxy fields.",
            "not": {
              "required": [
                "cloud_platforms",
                "galaxy_tags",
                "min_ansible_version",
                "namespace",
                "platforms",
                "role_name",
                "video_links"
              ]
            },
            "required": [ "description" ]
          }
        }
      ],
      "else": {
        "$comment": "If standalone is false, then we have a collection role and only description is required",
        "required": [ "description" ]
      },
      "properties": {
        "description": {
          "title": "Description",
          "type": "string"
        },
        "author": {
          "title": "Author",
          "type": "string"
        },
        "cloud_platforms": {
          "markdownDescription": "Only valid for old standalone roles."
        },
        "company": {
          "title": "Company",
          "type": "string"
        },
        "galaxy_tags": {
          "title": "Galaxy Tags",
          "type": "array",
          "items": {
            "type": "string"
          },
          "markdownDescription": "See https://galaxy.ansible.com/docs/contributing/creating_role.html"
        },
        "github_branch": {
          "title": "GitHub Branch",
          "type": "string",
          "markdownDescription": "Optionally specify the branch Galaxy will use when accessing the GitHub repo for this role"
        },
        "issue_tracker_url": {
          "title": "Issue Tracker Url",
          "type": "string"
        },
        "license": {
          "title": "License",
          "type": "string"
        },
        "min_ansible_container_version": {
          "title": "Min Ansible Container Version",
          "type": "string"
        },
        "min_ansible_version": {
          "title": "Min Ansible Version",
          "type": "string"
        },
        "namespace": {
          "title": "Namespace Name",
          "type": "string",
          "pattern": "^[a-z][a-z0-9_]+$",
          "minLength": 2,
          "markdownDescription": "Used by molecule and ansible-lint to compute FQRN for roles outside collections"
        },
        "platforms": {
          "$ref": "#/definitions/platforms"
        },
        "role_name": {
          "title": "Role Name",
          "type": "string",
          "pattern": "^[a-z][a-z0-9_]+$",
          "minLength": 2
        },
        "standalone": {
          "title": "Standalone",
          "description": "Set to true for old standalone roles, or false for new collection roles.",
          "type": "boolean"
        },
        "video_links": {
          "type": "array",
          "markdownDescription": "Only valid for old standalone roles."
        }
      },
      "additionalProperties": false
    },
